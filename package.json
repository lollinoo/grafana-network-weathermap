{
  "name": "knightss-27-d-3-weathermap",
  "version": "0.4.3",
  "description": "Network weathermap",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "audit:prod": "npm audit --omit=dev --audit-level=high",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "npx --yes cypress@latest install && npx --yes @grafana/e2e@latest run",
    "e2e:update": "npx --yes cypress@latest install && npx --yes @grafana/e2e@latest run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint --fix",
    "posttest": "make-coverage-badge",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "verify:ci": "npm run lint && npm run typecheck && npm run test:ci && npm run build"
  },
  "repository": "github:knightss27/grafana-network-weathermap",
  "author": "seth@seth.cx",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/knightss27/grafana-network-weathermap/issues",
    "email": "seth@seth.cx"
  },
